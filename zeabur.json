{
  "name": "crypto-data-collector",
  "services": [
    {
      "name": "api",
      "type": "docker",
      "dockerfile": "Dockerfile",
      "ports": [
        {
          "port": 5001,
          "protocol": "http"
        }
      ],
      "env": {
        "PYTHONUNBUFFERED": "1"
      },
      "volumes": [
        {
          "name": "data",
          "mountPath": "/app/data"
        }
      ]
    }
  ]
}
